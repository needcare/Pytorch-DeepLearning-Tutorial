    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "i7cTE5wm-euH",
        "outputId": "e8d40bda-af91-41fc-d6bb-f8461c2632b8"
      },
      "source": [
        "tensor_a>0"
